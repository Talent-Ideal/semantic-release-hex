{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!tests/fixtures/**",
    "!**/node_modules/**",
    "!**/coverage/**"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 100,
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  },
  "transform": {}
}
